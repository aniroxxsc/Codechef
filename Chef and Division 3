#include<iostream>
using namespace std;

int main(){
    int t;
    cin>>t;
    while(t--){
        int n,k,d;
        cin>>n>>k>>d;
        int arr[n];
        int sum=0;
        for(int i=0;i<n;i++){
            cin>>arr[i];
            sum+=arr[i];
        }
        sum =sum/k;
        if(sum<=d){
            cout<<sum<<endl;
        }
        else{
            cout<<d<<endl;
        }
    }
    return 0;
}

